{
  "name": "@xpadev-net/material-icons",
  "version": "0.0.1",
  "description": "material-icons component for react without extra dependencies",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "scripts": {
    "build": "npx rimraf dist&&npx copyfiles -u 1 ./static/**/*.json ./dist/&&node ./utils/replace-version.mjs&&npm run build:ts&&npm run build:dts",
    "build:ts": "tsup",
    "build:dts": "rollup -c rollup.config.dts.mjs",
    "watch": "rollup -c rollup.config.mjs -w",
    "typedoc": "typedoc --entryPointStrategy Expand --out ./docs/type/ ./src/",
    "prepublishOnly": "npm run build",
    "check-types": "npx tsc --noEmit",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{tsx,ts,js,json,css,scss}\"",
    "lint": "npm run eslint&&npm run check-types",
    "lint:fix": "npm run format&&npm run eslint:fix&&npm run check-types",
    "update-icons": "ts-node --esm -r tsconfig-paths/register src/utils/update.ts",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "publish:all": "cd dist/icons&&pnpm publish&&cd ../symbols&&pnpm publish",
    "prepublish": "exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xpadev-net/material-icons.git"
  },
  "keywords": [],
  "author": "xpadev(xpadev.net)",
  "bugs": {
    "url": "https://github.com/xpadev-net/material-icons/issues"
  },
  "files": [
    "dist/index.js",
    "dist/index.d.ts"
  ],
  "homepage": "https://github.com/xpadev-net/material-icons/issues",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@babel/preset-env": "^7.22.7",
    "@babel/preset-react": "^7.22.5",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-image": "^3.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "^11.1.2",
    "@storybook/addon-essentials": "^7.0.26",
    "@storybook/addon-interactions": "^7.0.26",
    "@storybook/addon-links": "^7.0.26",
    "@storybook/blocks": "^7.0.26",
    "@storybook/react": "^7.0.26",
    "@storybook/react-vite": "^7.0.26",
    "@storybook/testing-library": "0.0.14-next.2",
    "@swc/core": "^1.3.73",
    "@types/node": "^20.4.1",
    "@types/react": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "concurrently": "^8.2.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsdoc": "^44.2.7",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "lite-server": "^2.6.1",
    "prettier": "2.8.8",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.26.2",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript-paths": "^1.4.0",
    "storybook": "^7.0.26",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.0",
    "tsup": "^7.2.0",
    "typedoc": "^0.24.8",
    "typedoc-plugin-missing-exports": "^1.0.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.2"
  },
  "dependencies": {
    "react": "^18.2.0"
  }
}
